    @*@Scripts.Render("~/bundles/routeCygnus")
    @Scripts.Render("~/bundles/scriptCygnus")*@

    @*@Styles.Render("~/Content/css")
        @Scripts.Render("~/bundles/modernizr")*@

    <style type="text/css">
        #loading {
            display: none;
            position: fixed;
            left: 0;
            top: 0;
            width: 100%;
            height: 100%;
            background: rgba(192,192,192,0.3);
            z-index: 1000;
        }

        #loadingcontent {
            display: table;
            position: fixed;
            left: 0;
            top: 0;
            width: 100%;
            height: 100%;
        }

        #loadingspinner {
            display: table-cell;
            vertical-align: middle;
            width: 100%;
            text-align: center;
            font-size: larger;
            padding-top: 80px;
        }
    </style>

</head>
<body class="hold-transition sidebar-mini layout-fixed">
    <div class="wrapper">
        <nav class="main-header navbar navbar-expand navbar-white navbar-light navCygnus">
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
                </li>
            </ul>
            <ul class="navbar-nav ml-auto">
                <li class="nav-item dropdown">
                    <a class="nav-link" data-toggle="dropdown" href="#">
                        <i class="far fa-bell"></i>
                        <span id="spanCantidadNotif" class="badge navbar-badge accionCygnus"></span>
                    </a>
                    <div id="divNotificaciones" class="dropdown-menu dropdown-menu-lg dropdown-menu-right">
                        <span id="spanNumNot" class="dropdown-item dropdown-header"></span>
                        <div class="dropdown-divider"></div>

                    </div>
                </li>
                <li class="nav-item dropdown">
                    <a class="nav-link" data-toggle="dropdown" href="#">
                        <i class="fas fa-th-large"></i>
                    </a>
                    @*<div class="dropdown-menu dropdown-menu-lg dropdown-menu-right">
                            <div class="dropdown-menu dropdown-menu-lg dropdown-menu-right show">
                                <a href="https://sitios.cygnus-est.cl/login/login.aspx" class="dropdown-item">
                                    <i class="fas fa-times-circle text-red mr-2"></i> Cerrar Sesión
                                </a>
                            </div>
                        </div>*@
                    <div class="dropdown-menu dropdown-menu-lg dropdown-menu-right">
                        <a href="~/logout.aspx?opcion=1" class="dropdown-item">
                            <i class="fas fa-reply text-blue mr-2"></i> Volver a Sistemas
                        </a>
                        <div class="dropdown-divider"></div>
                        <a href="~/logout.aspx" class="dropdown-item">
                            <i class="fas fa-times-circle text-red mr-2"></i> Cerrar Sesión
                        </a>
                    </div>
                </li>
            </ul>
        </nav>
        <aside class="main-sidebar sidebar-dark-primary elevation-4 menuCygnus" data-visible="true">
            <a href="#" class="brand-link" onclick="home();">
                <img src="/Content/images/cygnus.png" alt="AdminLTE Logo" class="brand-image img-circle elevation-3">
                <span class="brand-text font-weight nombreSistema">Super sistema</span>
            </a>
            <div style="padding: 0px 8px; height: 100%; width: 100%;">
                <div class="user-panel mt-3 pb-3 mb-3 d-flex">
                    <div class="image">
                        <img src="@ViewBag.fotoUsuario" class="img-circle elevation-2" alt="User Image">
                    </div>
                    <div class="info">
                        <a href="#" class="d-block" onclick="home();">@ViewBag.nomUsuario</a>
                    </div>
                    <input type="hidden" id="rut" name="rut" value="@ViewBag.rutUsuario" />
                    <input type="hidden" id="usr" name="usr" value="@ViewBag.usrUsuario" />
                    <input type="hidden" id="uxs" name="uxs" value="@ViewBag.uxsUsuario" />
                    <input type="hidden" id="ulegajo" name="ulegajo" value="@ViewBag.legajoUsuario" />
                    <input type="hidden" id="nombre" name="nombre" value="@ViewBag.nomUsuario" />
                    <input type="hidden" id="token" name="token" value="@ViewBag.tokUsuario" />
                    <input type="hidden" id="ucta" name="ucta" value="@ViewBag.ctaUsuario" />
                </div>

                <nav class="mt-2">
                    <ul id="menuCygnus" class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                    </ul>
                </nav>
            </div>
        </aside>

        <div id="body" class="content-wrapper">
            @RenderBody()
        </div>

        <div id="loading">
            <div id="loadingcontent">
                <p id="loadingspinner">
                    Se están procesando los datos...
                </p>
            </div>
        </div>

        <footer class="main-footer">
            <strong>Copyright &copy; 2022 <a href="https://www.cygnus.cl">Cygnus</a>.</strong>
            Todos los derechos reservados.
            <div class="float-right d-none d-sm-inline-block">
                <b>Version</b> 1.0.0
            </div>
        </footer>

        <aside class="control-sidebar control-sidebar-dark"></aside>
    </div>

