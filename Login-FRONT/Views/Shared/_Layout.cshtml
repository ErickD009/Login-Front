    <link rel="stylesheet" type="text/css" href="~/Content/themeCygnus/plugins/fullcalendar-daygrid/main.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/themeCygnus/plugins/fullcalendar-timegrid/main.min.css">
